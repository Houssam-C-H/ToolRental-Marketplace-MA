# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here

# API Key - Use publishable key (new, recommended) or anon key (legacy)
# Publishable keys start with 'sb_publishable_' and offer improved security
# Anon keys start with 'eyJ' and still work but are being phased out
VITE_SUPABASE_ANON_KEY=your_supabase_publishable_key_or_anon_key_here
# Alternative: Use this if you prefer the new naming
# VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here

# Optional: Server Configuration
PORT=3000
NODE_ENV=development

# Optional: Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password 